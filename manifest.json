{
    "manifest_version": 3,
    "name": "알고노트 (AlgoNote)",
    "description": "편리하게 알고리즘 오답노트를 작성해보세요.",
    "homepage_url": "https://algnote.duckdns.org/home",
    "version": "1.0",
    "author": "euny0ung",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "action": {
        "default_icon": "images/logo.png",
        "default_title": "AlgoNote",
        "default_popup": "popup/popup.html"
    }
}
